{
    "manifest_version": 3,
    "name": "1-800-call-that-shit-in",
    "version": "0.0.0",
    "description": "Cut out the middle man and just call that shit in",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["tabs", "activeTab"],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://*.grubhub.com/*"],
            "js": ["scripts/index.js"]
        }
    ]
}
